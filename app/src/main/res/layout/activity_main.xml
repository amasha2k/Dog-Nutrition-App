<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/im4"
    tools:context=".MainActivity">

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="88dp"
        android:layout_marginEnd="16dp"
        android:backgroundTint="#433B2F"
        android:onClick="Profile"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@android:drawable/ic_menu_preferences" />

    <Button
        android:id="@+id/btnExit"
        android:layout_width="142dp"
        android:layout_height="38dp"
        android:layout_marginStart="253dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="16dp"
        android:backgroundTint="#E91E63"
        android:onClick="Logout"
        android:text="Logout"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btnProducts"
        android:layout_width="190dp"
        android:layout_height="53dp"
        android:layout_marginStart="110dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="111dp"
        android:backgroundTint="#FF9800"
        android:onClick="allProducts"
        android:text="All Products"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/emailTextView" />

    <Button
        android:id="@+id/btnOther"
        android:layout_width="190dp"
        android:layout_height="53dp"
        android:layout_marginStart="110dp"
        android:layout_marginTop="224dp"
        android:layout_marginEnd="111dp"
        android:backgroundTint="#FF9800"
        android:onClick="viewOther"
        android:text="Other"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/emailTextView" />

    <Button
        android:id="@+id/btnReview"
        android:layout_width="190dp"
        android:layout_height="53dp"
        android:layout_marginStart="110dp"
        android:layout_marginTop="416dp"
        android:layout_marginEnd="111dp"
        android:backgroundTint="#FF9800"
        android:onClick="Review"
        android:text="Review"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/emailTextView" />

    <Button
        android:id="@+id/btnArticles"
        android:layout_width="190dp"
        android:layout_height="53dp"
        android:layout_marginStart="110dp"
        android:layout_marginTop="288dp"
        android:layout_marginEnd="111dp"
        android:backgroundTint="#FF9800"
        android:onClick="Article"
        android:text="Articles"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/emailTextView" />

    <Button
        android:id="@+id/btnGuidelines"
        android:layout_width="190dp"
        android:layout_height="53dp"
        android:layout_marginStart="110dp"
        android:layout_marginTop="476dp"
        android:layout_marginEnd="111dp"
        android:backgroundTint="#FF9800"
        android:onClick="Guide"
        android:text="Guidelines"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/emailTextView" />

    <Button
        android:id="@+id/btnReview1"
        android:layout_width="190dp"
        android:layout_height="53dp"
        android:layout_marginStart="110dp"
        android:layout_marginTop="536dp"
        android:layout_marginEnd="111dp"
        android:backgroundTint="#FF9800"
        android:onClick="Review1"
        android:text="View Reviews"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/emailTextView" />

    <Button
        android:id="@+id/btnBuy"
        android:layout_width="190dp"
        android:layout_height="53dp"
        android:layout_marginStart="110dp"
        android:layout_marginTop="356dp"
        android:layout_marginEnd="111dp"
        android:backgroundTint="#FF9800"
        android:onClick="Buy"
        android:text="Buy"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/emailTextView" />

    <Button
        android:id="@+id/btnNutrition"
        android:layout_width="190dp"
        android:layout_height="53dp"
        android:layout_marginStart="110dp"
        android:layout_marginTop="160dp"
        android:layout_marginEnd="111dp"
        android:backgroundTint="#FF9800"
        android:onClick="viewNutrition"
        android:text="Nutrition"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/emailTextView" />

    <Button
        android:id="@+id/btnFood"
        android:layout_width="190dp"
        android:layout_height="53dp"
        android:layout_marginStart="110dp"
        android:layout_marginTop="96dp"
        android:layout_marginEnd="111dp"
        android:backgroundTint="#FF9800"
        android:onClick="viewFood"
        android:text="Food"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/emailTextView" />

    <TextView
        android:id="@+id/emailTextView"
        android:layout_width="220dp"
        android:layout_height="39dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="100dp"
        android:text="Welcome: "
        android:textColor="#D3CCCC"
        android:textSize="16sp"
        android:textStyle="bold"
        android:typeface="serif"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="420dp"
        android:layout_height="53dp"
        android:layout_marginTop="13dp"
        android:fontFamily="@font/aclonica"
        android:text="Dog Nutrition App"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="34sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>